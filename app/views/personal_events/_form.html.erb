   <div class="col-md-6">
     <%= simple_form_for @personal_event do |f| %>
      <div class="form-group">
         <%= f.label :description %>
         <%= f.text_field :description, :value => @personal_event.snip_off_person , autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
         <%= f.label :date %>
         <%= f.date_field :starting, class: 'form-control' %>
      </div>
      <div class="form-group">
         <%= f.label :end_date %>
         <%= f.date_field :ending, :value => nil, class: 'form-control' %>
      </div>
      <div class="form-group">
         <%= f.label :private %>
         <%= f.check_box :private%>
      </div>
      <div class="form-group links">
        <strong>Tags</strong> <%= link_to_add_association 'Add Tag', f, :tags, {"data-association-insertion-method" => 'after'} %>
      </div>
      <div class="col-md-12" %>
        <div class="form-group" id="tags">
          <%= f.simple_fields_for :tags do |tag| %>
            <%= render 'tag_fields', f: tag %>
          <% end %>
        </div>
      </div>
      <div class-"col-md-12" %>
      <%= f.hidden_field :person_id %>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
         <%= link_to "DELETE", remove_personal_event_path(id: @personal_event.id), method: :delete, data: { confirm: 'Are you sure you want to delete this event?' }, class: 'btn btn-danger' %>
       </div>
     <% end %>
   </div>