   <div class="col-md-6">
     <%= simple_form_for @personal_event do |f| %>
      <div class="form-group">
         <%= f.label :description %>
         <%= f.text_field :description, class: 'form-control', placeholder: "Description" %>
      </div>
      <div class="form-group">
         <%= f.label :starting %>
         <%= f.date_field :starting, class: 'form-control', placeholder: "Starting Date" %>
      </div>
        <div class="form-group">
         <%= f.label :ending %>
         <%= f.date_field :ending, class: 'form-control', placeholder: "Ending Date" %>
      </div>
      <div class="form-group">
         <%= f.label :private %>
         <%= f.check_box :private%>
      </div>
      <div class="form-group links">
        <strong>Tags</strong> <%= link_to_add_association 'Add Tag', f, :tags, {"data-association-insertion-method" => 'after'} %>
      </div>
      <div class="col-md-12" %>
        <div class="form-group" id="tags">
          <%= f.simple_fields_for :tags do |tag| %>
            <%= render 'tag_fields', f: tag %>
          <% end %>
        </div>
      </div>
      <div class-"col-md-12" %>
      <%= f.hidden_field :person_id %>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
   </div>