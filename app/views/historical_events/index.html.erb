<%= render 'navigation' %>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<div class="row">
  <div class="col-md-12">
    <%= current_user %>
    <table class="table table-striped table-hover">
    <% @events.each do |event| %>
      <tr>
        <td><%= event.starting %></td><td><%= event.description %></td><td><%= event.ending %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
