<%= render 'navigation' %>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
    <% @people.each do |person| %>
      <tr>
        <td><%= person.born %><br><%= person.died %></td>
        <td><%= link_to person.name, person %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>